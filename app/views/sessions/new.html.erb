<%= form_for :session, url: login_path do |f| %>
  <%= f.label :email %>
  <%= f.text_field :email %>
  <%= f.label :password %>
  <%= f.password_field :password %>
  <%= f.submit "Log In", class: 'btn btn-success' %>
<% end %>
<%= link_to "Create Account", new_user_path, class: 'btn btn-success' %>
